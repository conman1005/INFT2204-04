extends ../index
block content
    table.table.table-striped
        tr
            th Name
            th Email
            th Time of Registration
            th Is Active
            th Friends List
            th Address
        each user in users 
            tr
                td=user.name   
                td=user.email   
                td=user.createdAt
                td=user.isActive
                td=user.friends 
                td=user.address.street + ", " + user.address.city + ", " + user.address.state  + ", " + user.address.zip
